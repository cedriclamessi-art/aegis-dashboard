{
  "name": "aegis-v5.0",
  "version": "5.0.0",
  "description": "AEGIS v5.0 - Production-Ready AI Agent Platform",
  "main": "dist/api/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:dashboard\"",
    "dev:api": "tsx watch api/server.ts",
    "dev:dashboard": "npm --prefix frontend run dev",
    "build": "npm run build:api && npm run build:dashboard",
    "build:api": "tsc",
    "build:dashboard": "npm --prefix frontend run build",
    "start": "node dist/api/server.js",
    "start:all": "concurrently \"npm start\" \"npm run preview:dashboard\"",
    "preview:dashboard": "npm --prefix frontend run preview",
    "db:init": "tsx scripts/init-db.ts",
    "db:seed": "tsx scripts/seed-agents.ts",
    "agents:validate": "tsx scripts/validate-agents.ts",
    "connectors:test": "tsx scripts/test-connectors.ts",
    "test": "vitest run",
    "health-check": "tsx scripts/health-check.ts"
  },
  "dependencies": {
    "pg": "^8.11.0",
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "uuid": "^9.0.0",
    "pino": "^8.17.0",
    "node-cron": "^3.0.2",
    "bull": "^4.12.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^1.6.0",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
